{
    "compilerOptions": {
        "strict": true,
        "noImplicitAny": true,
        "esModuleInterop": true,
        "strictNullChecks": true,
        "target": "ES6", // we have to lok this 
        "moduleResolution": "Node10",
        "module": "CommonJS",
        "declaration": true,
        "isolatedModules": true,
        "noEmit": true,
        "outDir": "dist",
        "types": [ "node" ],
        
    
    },
    "include": ["src"],
    "exclude": ["node_modules"],
}